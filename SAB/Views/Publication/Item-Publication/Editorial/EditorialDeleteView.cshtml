
@{
    ViewBag.Title = "EditorialDeleteView";
}

<div class="row">
    <div class="col-md-12 title">
        <h1>Borrar Suscripción</h1>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <ol class="breadcrumb">
            <li>@Html.ActionLink("Home", "Index", "Home")</li>
            <li>@Html.ActionLink("Suscripcion", "SearchSuscription", "Suscripcion")</li>
            <li>@Html.ActionLink("Buscar", "SearchSuscription", "Suscripcion")</li>
            <li>@Html.ActionLink("Detalle", "DetailView", "Suscripcion")</li>
            <li class="active">Borrar </li>
        </ol>
    </div>
</div>

<div class="col-md-10">
    <div class="form-group">
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label for="searchText">Cód. Editorial</label>
                    <input class="form-control" id="disabledImput" type="text" placeholder="E003" disabled>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label for="searchText">Fecha de Registro</label>
                    <input class="form-control" id="disabledImput" type="text" placeholder="25/09/2014" disabled>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label for="searchText">Razón Social</label>
                    <input class="form-control" id="disabledImput" type="text" placeholder="Gestión y Sistemas" disabled>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label for="searchText">Estado</label>
                    <input class="form-control" id="disabledImput" type="text" placeholder="Activo" disabled>
                </div>
            </div>
        </div>
    </div>
    <div class="form-group">
        <input type="button" class="btn btn-primary" value="Cancelar" onclick="window.location.href = 'EditorialShow'" />
        <div class="col-sm-4">
            <input type="button" class="btn btn-primary pull-left" value="Borrar" onclick="window.location.href='EditorialList'" />
        </div>

    </div>
</div>


