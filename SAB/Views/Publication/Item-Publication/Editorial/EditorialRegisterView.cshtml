    <div class="form-group">
        <div class="row">
            <div class="col-md-1">
            </div>
            <div class="col-md-3">
                <label for="tipo">RUC :</label>
            </div>
            <div class="col-md-8">
                <div class="form-group">
                    <input type="text" name="searchText" value=" " placeholder="Search..." class="form-control">
                </div>
            </div>
        </div>
    </div>

    <div class="form-group">
        <div class="row">
            <div class="col-md-1">
            </div>
