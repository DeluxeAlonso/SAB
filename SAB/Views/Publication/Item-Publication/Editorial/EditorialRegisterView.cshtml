}

<div class="row">
    <div class="col-md-12">
        <ol class="breadcrumb">
            <li>@Html.ActionLink("Home", "Index", "Home")</li>
            <li>@Html.ActionLink("Editorial", "EditorialRegister", "Editorial")</li>
            <li class="active">Registrar</li>
        </ol>
    </div>
</div>

